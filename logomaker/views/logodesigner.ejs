<script type="text/javascript">
    $(function () {
        $('.template-list li').hover(function () {
            $(this).addClass('active');
            $(this).find('.flip-template').fadeIn(300);
        }, function () {
            $(this).find('.flip-template').fadeOut(100);
            $(this).removeClass('active');
        });
        $('.template-filter-li').on('click', function () {
            $('.template-filter-li').removeClass('active');
            $(this).addClass('active');
        });
        $("#e1").select2();
        $('.panel-title-active a').on('click', function () {
            $('#company-name').val("");
            $(this).parents(':eq(1)').next().slideDown();
            $(this).parents(':eq(1)').next().addClass('in');
            if ($('#accordionOne .panel-body').is(':visible')) {
                $('.identity-next').hide();
                $('.identity-cancel').css('display', 'inline-block');
                $('.wizard-steps li').removeClass('active');
                $('.wiz-one').addClass('active');
                $('#company-name').focus();
            }
            else if ($('#accordionTwo .panel-body').is(':visible')) {
                $('.wizard-steps li').removeClass('active');
                $('.wizard-steps li.wiz-two').addClass('active');
                $('#industry-key-search').val("");
                $('.keyword-lists li').show();
                $('#industry-key-search').focus();
            }
            else if ($('#search .panel-body').is(':visible')) {
                $('.wizard-steps li').removeClass('active');
                $('.wizard-steps li.wiz-two').addClass('active');
                $('#industry-sub-key-search').val("");
                $('.keyword-lists li').show();
                $('#industry-sub-key-search').focus();
            }
            else if ($('#accordionThreeaccordionThree .panel-body').is(':visible')) {
                $('.wizard-steps li').removeClass('active');
                $('.wizard-steps li.wiz-three').addClass('active');
            }
            else if ($('#accordionFour .panel-body').is(':visible')) {
                $('.wizard-steps li').removeClass('active');
                $('.wiz-four').addClass('active');
            }
        });

        var windowH = $(window).height();
        $(window).scroll(function () {
            $('.create-logo-footer').show();
        });
    });
</script>

